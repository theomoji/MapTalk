<% form_for @user do |f|%>
<%= f.label :name %>
<%= f.text_field :name %></br>


<%= f.label :email %>
<%= f.text_field :email %></br>

<%= f.label :username %>
<%= f.text_field :username %></br>

<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.submit "Sign Up!" %>
<%= f.submit 'Back', :back %>
<% end %>
